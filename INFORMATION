# Make first admin (register on website>supabase SQL editor and run this)
-- First, lets verify the user exists and get their UUID
SELECT id, email 
FROM auth.users 
WHERE email = 'vr382@cam.ac.uk';

-- After confirming the ID, update their role to admin
UPDATE profiles 
SET role = 'admin' 
WHERE id = (
  SELECT id 
  FROM auth.users 
  WHERE email = 'vr382@cam.ac.uk'
);

-- Verify the change
SELECT profiles.*, auth.users.email 
FROM profiles 
JOIN auth.users ON profiles.id = auth.users.id 
WHERE auth.users.email = 'vr382@cam.ac.uk';






# DELETING AN ADMIN

-- 1. First get or note the admin's user ID
-- In your case it's: '1cecfa29-60b0-404b-866d-d5ce48703cc3'

-- 2. Update any inventory items created by this admin to be owned by another admin
-- Replace 'new-admin-id' with the ID of another admin user
UPDATE inventory 
SET created_by = 'new-admin-id' 
WHERE created_by = '1cecfa29-60b0-404b-866d-d5ce48703cc3';

-- 3. Delete their logs
DELETE FROM inventory_logs 
WHERE user_id = '1cecfa29-60b0-404b-866d-d5ce48703cc3';

-- 4. Delete their bookings
DELETE FROM inventory_bookings 
WHERE user_id = '1cecfa29-60b0-404b-866d-d5ce48703cc3';

-- 5. Delete their profile
DELETE FROM profiles 
WHERE id = '1cecfa29-60b0-404b-866d-d5ce48703cc3';

-- 6. Finally delete the user
DELETE FROM auth.users 
WHERE id = '1cecfa29-60b0-404b-866d-d5ce48703cc3';